{
  "name": "functions",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "concurrently \"npm run build:watch\" \"firebase emulators:start --inspect-functions --import=./.data --export-on-exit\"",
